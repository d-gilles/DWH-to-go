{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:*",
          "iam:CreateRole",
          "iam:AttachRolePolicy",
          "iam:PassRole",
          "iam:DeleteRole",
          "iam:DetachRolePolicy",
          "logs:*",
          "cloudwatch:*"
        ],
        "Resource": "*"
      }
    ]
  }
  